    "coveralls": "^3.1.1",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.2.1",
