    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.3.2",
    "jest-fetch-mock": "^3.0.3",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "node-fetch": "2",
    "prom-client": "^14.2.0",
    "rate-limiter-flexible": "^2.4.1",
    "winston": "^3.8.2",
    "yarn": "^1.22.19"
  },
